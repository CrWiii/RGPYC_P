	$(document).on('change','#notifier_type_id',function(){
		var quienid = $(this).val();
		if(quienid==1){
			$('#notificacion_tab').hide();
		}
		else{
			$('#notificacion_tab').show();
		}
	});

	$(document).on('click','#searchPerson',function(){
		person_type = $(this).attr('data-id');
		console.log(person_type);
		var param = [];
		switch(person_type){ 
			case '1': param = notifier; break;
			case '2': param = confirming_who; break;
			case '3': param = ejecutivo_cia; break;
			case '4': param = ejecutivo_broker; break;
			case '5': param = asegurado; break;
			case '6': param = contact_contratante; break;
			case '7': param = ajustador_asignado; break;
			case '8': param = contact_inspeccion; break;
			case '9': param = equipo; break;
		}		
		console.log(param);
		if(param != ''){
			$('#nombres').val(param.data.nombres).attr('disabled','disabled');
			$('#apellido_paterno').val(param.data.apellido_paterno).attr('disabled','disabled');
			$('#apellido_materno').val(param.data.apellido_materno).attr('disabled','disabled');
			
			if(param.data.email!=='' && param.data.email!==null){$('#email').val(param.data.email).attr('disabled','disabled');
				}else{$('#email').val('').removeAttr('disabled','disabled');}
			if(param.data.telefono!=='' && param.data.telefono!==null){$('#telefono').val(param.data.telefono).attr('disabled','disabled');
				}else{$('#telefono').val('').removeAttr('disabled','disabled');}
			if(param.data.cargo!=='' && param.data.cargo!==null){$('#cargo').val(param.data.cargo).attr('disabled','disabled');
				}else{$('#cargo').val('').removeAttr('disabled','disabled');}
			if(param.data.fecha_nacimiento!='' && param.data.fecha_nacimiento!==null){$('#fecha_nacimiento').val(param.data.fecha_nacimiento).attr('disabled','disabled');
				}else{$('#fecha_nacimiento').val('').removeAttr('disabled','disabled');}
		}
		
		// $('#dni,#nombres,#apellido_paterno,#apellido_materno,#email,#telefono,#cargo,#fecha_nacimiento').val('').removeAttr('disabled','disabled');
		$('#modal-default').modal('show');
	});

	$(document).on('click','#closeModalPerson',function(){
		$('#dni,#nombres,#apellido_paterno,#apellido_materno,#email,#telefono,#cargo,#fecha_nacimiento').val('').removeAttr('disabled','disabled');
	});

	$(document).on('click','#searchDNI',function(){
		$('#nombres,#apellido_paterno,#apellido_materno,#email,#telefono,#cargo,#fecha_nacimiento').val('').removeAttr('disabled','disabled');

		var dnival = $('#dni').val();
		if(dnival.length == 8){
			$.ajax({
				type:'post',
				url: '/searchPers',
				data: {'_token': $('input[name=_token]').val(),'dni':dnival},
				success:function(data){
					// console.log(data);
					switch(person_type){
						case '1': notifier = data; break;
						case '2': confirming_who = data; break;
						case '3': ejecutivo_cia = data; break;
						case '4': ejecutivo_broker = data; break;
						case '5': asegurado = data; break;
						case '6': contact_contratante = data; break;
						case '7': ajustador_asignado = data; break;
						case '8': contact_inspeccion = data; break;
						case '9': equipo = data; break;

					}
					// console.log(notifier);
					$('#nombres').val(data.data.nombres).attr('disabled','disabled');
					$('#apellido_paterno').val(data.data.apellido_paterno).attr('disabled','disabled');
					$('#apellido_materno').val(data.data.apellido_materno).attr('disabled','disabled');
					
					if(data.data.email!=='' && data.data.email!==null){$('#email').val(data.data.email).attr('disabled','disabled');}
					if(data.data.telefono!=='' && data.data.telefono!==null){$('#telefono').val(data.data.telefono).attr('disabled','disabled');}
					if(data.data.cargo!=='' && data.data.cargo!==null){$('#cargo').val(data.data.cargo).attr('disabled','disabled');}
					if(data.data.fecha_nacimiento!='' && data.data.fecha_nacimiento!==null){$('#fecha_nacimiento').val(data.data.fecha_nacimiento).attr('disabled','disabled');}
					person_id_selected = data.data.id;
					person_fn_selected =data.data.search;
				},
			}).done(function(){
	  		}).fail(function(){

	  			var tecactusApi = new TecactusApi("eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiIsImp0aSI6IjJkMmQxN2EwM2ZmNDlhODg5YzRjMjhiNjQzNDMwNGY4MjkzMTMzZjRmZDI0MWUxZWNlYzQ5MzUyOTRhYzcxMGI2MGI0ZDU5MzFkZjZlOWI0In0.eyJhdWQiOiIxIiwianRpIjoiMmQyZDE3YTAzZmY0OWE4ODljNGMyOGI2NDM0MzA0ZjgyOTMxMzNmNGZkMjQxZTFlY2VjNDkzNTI5NGFjNzEwYjYwYjRkNTkzMWRmNmU5YjQiLCJpYXQiOjE0OTM3NjAwNTUsIm5iZiI6MTQ5Mzc2MDA1NSwiZXhwIjoxODA5MjkyODU1LCJzdWIiOiIyMzciLCJzY29wZXMiOlsidXNlLXJlbmllYyIsInVzZS1zdW5hdCJdfQ.XAuMrb7UeXY3_-y0CyNJ5Kblv-ADhBtkMeqAgYFpDJBYjd4n6m37kBxoMYVTbd-Z1XdXWRqFWBq7YaHYRvFYDtezIe_bGUpIMHmXO469_X62P5JrX6bURx7MqYKNJRdBupPMBIaOqw6qGXubbMhoeV4r4QWTwQnBuzi4D3SnRfPydn-WIuju75zLBSjKF3cJUObhLFXFhbYTevr5RGihPyc1kNDM1JZTV4miAs_N56xnnLvovp2kSdFVwSVzOur35UDhiUUC5tXuTFxYb2yyEHspQmIqMtWbR97u8vRy5_W2-OWSetiim8vAp0SuS2el8tDOSG-np5chiWRQiYM3X1Mah-EFtM_FRt16Y67F0hmEIz3SHvGfyw5K44kh_0ricolwb6gK32Nq30nqn2mVegEi3MzSLDtWguWl8Z8SzILyR3hEbsN2x5uCxbv1eTYAoPd8g6H2vYlpVpaKNEje6fMfBSaYUS68GG18mf_uACqGxwA9ImPplkv_gJdTdxdcaSGp2ZKmn7qNB5HOTm3IwEcSbvUUBHKJ1eYzLuS9o4cTpzGPKq35Bojrif1kbwimcdxDx2dwbygMNDBada2egttyl388abD14eYlWACOcsVcoqRPGDiiXGvJjBmaE6vElVfTfbdqVvuD8c5JEPnDga_Hej5z9xxpPJC1YYURZGQ")

				tecactusApi.Reniec.getDni(dnival)
