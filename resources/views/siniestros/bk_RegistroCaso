	});


	$(function () {
			$('#datepicker,#confirming_date,#fecha_vitacora ,#notifier_date,#fecha_siniestro,#fecha_coordinacion_inspeccion,#fecha_programada_inspeccion,#fecha_iforme_final,#fecha_realizacion').datetimepicker({
